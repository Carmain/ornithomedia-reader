{
  "manifest_version": 2,

  "name": "Ornithomedia reader",
  "description": "Change the way to display the articles in ornithomedia",
  "version": "0.1",
  "author": "Martin Billard",

  "permissions": [
    "http://www.ornithomedia.com/*"
  ],

  "content_scripts": [
    {
      "matches": ["http://www.ornithomedia.com/*/*/*.html"],
      "js": ["clean.js"],
      "run_at": "document_end"
    }
  ],

  "browser_action": {
    "default_icon": "icons/icon.png"
  }
}
